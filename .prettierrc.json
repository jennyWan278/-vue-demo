{
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": false,
	"semi": true,
	"singleQuote": true,
	"proseWrap": "preserve",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"eslintIntegration": false,
	"htmlWhitespaceSensitivity": "ignore",
	"ignorePath": ".prettierignore",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": true,
	"parser": "babylon",
	"requireConfig": false,
	"stylelintIntegration": false,
	"trailingComma": "none",
	"tslintIntegration": false
}
